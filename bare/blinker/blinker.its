/dts-v1/;

/ {
	description = "Bare Metal LED blinker for RPI 3";
	#address-cells = <1>;

	images {
		kernel@1 {
			description = "Blinker standalone binary";
			type = "kernel";
			data = /incbin/("./blinker.bin");
			os = "u-boot";
			arch = "arm";
			load = <0x8000>;
			entry = <0x8000>;
			compression = "none";
		};
	};
};
